<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorAccent">#33B5E5</item>
        <item name="colorPrimary">#2d8f77</item>
        <item name="colorPrimaryDark">#99D3BC</item>
        <item name="colorControlHighlight">#FF8800</item>
        <item name="colorControlNormal">#0099CC</item>

        <!--
        환경설정할때 테마 설정안하면 에러남
        Unhandled Exception: Java.Lang.IllegalStateException: Must specify preferenceTheme in theme occurred
        -->
        <item name="preferenceTheme">@style/PreferenceThemeOverlay</item>
    </style>

</resources>
